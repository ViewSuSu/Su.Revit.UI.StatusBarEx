<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <UseWPF>true</UseWPF>
	  <UseWindowsForms>true</UseWindowsForms>
	  <LangVersion>latest</LangVersion>
	  <Authors>Su</Authors>
	  <Version>1.0.0</Version>
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
	  <Configurations>RVT_18;RVT_19;RVT_20;RVT_21;RVT_22;RVT_23;RVT_24;RVT_18_D;RVT_19_D;RVT_20_D;RVT_21_D;RVT_22_D;RVT_23_D;RVT_24_D;RVT_17_D;RVT_17;RVT_16;RVT_16_D;RVT_25;RVT_25_D;RVT_26;RVT_26_D</Configurations>
